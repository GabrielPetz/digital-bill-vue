ls() {\n            return axios({\n                method: 'get',\n                url: 'http://localhost:8001/api/bill/',\n                withCredentials: false,\n                responseType: 'json',\n                // headers: {\n                //     'Access-Control-Allow-Origin': '*',\n                //     'Content-Type': 'application/json',\n                // },\n            })\n        },\n        getCategories() {\n            return axios({\n                method: 'get',\n                url: 'http://localhost:8001/api/category/',\n                responseType: 'json',\n                headers: {\n                    'Access-Control-Allow-Origin': '*',\n                    'Content-Type': 'application/json',\n                },\n            })\n        }\n    },\n    async created() {\n        Promise.all([this.getBills(), this.getCategories()])\n            .then(function (results) {\n                const bills = results[0];\n                const categories = results[1];\n\n                console.log(bills, categories)\n            });\n\n    }\n\n}\n</script>\n\n<style scoped>\n\n</style>\n\n<style>\nmain.mt-auto {\n    margin-top: 10% !important;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}