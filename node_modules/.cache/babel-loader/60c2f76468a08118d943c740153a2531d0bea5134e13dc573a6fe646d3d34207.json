{"ast":null,"code":"import axios from \"axios\";\naxios.defaults.headers.post['Content-Type'] = 'application/json;charset=utf-8';\nexport default {\n  name: 'ViewBill',\n  data() {\n    return {\n      bills: null,\n      categories: null\n    };\n  },\n  methods: {\n    query() {\n      console.log(\"aquio\");\n      let bills = axios.get(\"http://localhost:8001/api/bill/\");\n      console.log(bills);\n    },\n    getBills() {\n      return axios({\n        method: 'get',\n        url: 'http://localhost:8001/api/bill/',\n        withCredentials: false,\n        responseType: 'json'\n      });\n    },\n    async getCategories() {\n      return axios({\n        method: 'get',\n        url: 'http://localhost:8001/api/category/',\n        responseType: 'json'\n      });\n    }\n  },\n  async created() {\n    let [bills, categories] = [null, null];\n    Promise.all([this.getBills(), this.getCategories()]).then(function (results) {\n      bills = results[0];\n      categories = results[1];\n\n      // console.log(this.bills, this.categories)\n    });\n\n    console.log(bills);\n    console.log(categories);\n  }\n};","map":{"version":3,"mappings":"AAyBA,OAAOA,KAAI,MAAO,OAAO;AACzBA,KAAK,CAACC,QAAQ,CAACC,OAAO,CAACC,IAAI,CAAC,cAAc,IAAI,gCAAgC;AAE9E,eAAe;EACXC,IAAI,EAAE,UAAU;EAChBC,IAAI,GAAG;IACH,OAAO;MACHC,KAAK,EAAE,IAAI;MACXC,UAAU,EAAE;IAChB;EACJ,CAAC;EACDC,OAAO,EAAE;IACLC,KAAK,GAAG;MACJC,OAAO,CAACC,GAAG,CAAC,OAAO;MACnB,IAAIL,KAAI,GAAIN,KAAK,CAACY,GAAG,CAAC,iCAAiC;MACvDF,OAAO,CAACC,GAAG,CAACL,KAAK;IACrB,CAAC;IACDO,QAAQ,GAAG;MACP,OAAOb,KAAK,CAAC;QACTc,MAAM,EAAE,KAAK;QACbC,GAAG,EAAE,iCAAiC;QACtCC,eAAe,EAAE,KAAK;QACtBC,YAAY,EAAE;MAClB,CAAC;IACL,CAAC;IACD,MAAMC,aAAa,GAAG;MAClB,OAAOlB,KAAK,CAAC;QACTc,MAAM,EAAE,KAAK;QACbC,GAAG,EAAE,qCAAqC;QAC1CE,YAAY,EAAE;MAClB,CAAC;IACL;EACJ,CAAC;EACD,MAAME,OAAO,GAAG;IACZ,IAAI,CAACb,KAAK,EAAEC,UAAU,IAAI,CAAC,IAAI,EAAE,IAAI,CAAC;IACtCa,OAAO,CAACC,GAAG,CAAC,CACR,IAAI,CAACR,QAAQ,EAAE,EAAE,IAAI,CAACK,aAAa,EAAC,CACvC,CAAC,CAACI,IAAI,CAAC,UAAUC,OAAO,