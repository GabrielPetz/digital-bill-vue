            console.log(bills)\n        },\n        getBills() {\n            return axios({\n                method: 'get',\n                url: 'http://localhost:8001/api/bill/',\n                withCredentials: false,\n                responseType: 'json',\n            })\n        },\n        async getCategories() {\n            return axios({\n                method: 'get',\n                url: 'http://localhost:8001/api/category/',\n                responseType: 'json',\n            })\n        }\n    },\n    async created() {\n        let [bills, categories] = [null, null];\n        Promise.all([\n            this.getBills(), this.getCategories()\n        ]).then(function (results) {\n            bills = results[0];\n            categories = results[1];\n\n            // console.log(this.bills, this.categories)\n        });\n        console.log(bills)\n        console.log(categories)\n    }\n\n}\n</script>\n\n<style scoped>\n\n</style>\n\n<style>\nmain.mt-auto {\n    margin-top: 10% !important;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}