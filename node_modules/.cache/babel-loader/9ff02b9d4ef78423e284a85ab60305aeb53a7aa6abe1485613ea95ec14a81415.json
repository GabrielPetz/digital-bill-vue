{"ast":null,"code":"import ViewBillResult from \"../components/report/ViewBillResult\";\nimport ViewBillForm from \"../components/forms/ViewBillForm\";\nimport NoDataFound from \"../components/empty/NoDataFound.vue\";\nimport EmptyResult from \"../components/empty/EmptyResult.vue\";\nexport default {\n  name: 'ViewBill',\n  components: {\n    ViewBillResult,\n    ViewBillForm,\n    NoDataFound,\n    EmptyResult\n  },\n  data() {\n    return {\n      renderResult: null,\n      selectedBill: null,\n      selectedCategories: null\n    };\n  },\n  methods: {\n    reload() {\n      this.$forceUpdate();\n    },\n    getQueryEvent(kwargs) {\n      this.selectedBill = kwargs.selectedBill;\n      this.selectedCategories = kwargs.selectedCategories;\n      if (kwargs.selectedBill) {\n        if (this.renderResult === \"ViewBillResult\") {\n          this.reload();\n        }\n        this.renderResult = \"ViewBillResult\";\n      } else {\n        this.renderResult = \"EmptyResult\";\n      }\n    }\n  }\n};","map":{"version":3,"mappings":"AAUA,OAAOA,cAAa,MAAO,qCAAoC;AAC/D,OAAOC,YAAW,MAAO,kCAAiC;AAC1D,OAAOC,WAAU,MAAO,qCAAoC;AAC5D,OAAOC,WAAU,MAAO,qCAAoC;AAE5D,eAAe;EACXC,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE;IACRL,cAAc;IAAEC,YAAY;IAAEC,WAAW;IAAEC;EAC/C,CAAC;EACDG,IAAI,GAAG;IACH,OAAO;MACHC,YAAY,EAAE,IAAI;MAClBC,YAAY,EAAE,IAAI;MAClBC,kBAAkB,EAAE;IACxB;EACJ,CAAC;EACDC,OAAO,EAAE;IACLC,MAAM,GAAG;MACL,IAAI,CAACC,YAAY,EAAE;IACvB,CAAC;IACDC,aAAa,CAACC,MAAM,EAAE;MAClB,IAAI,CAACN,YAAW,GAAIM,MAAM,CAACN,YAAY;MACvC,IAAI,CAACC,kBAAiB,GAAIK,MAAM,CAACL,kBAAkB;MAGnD,IAAIK,MAAM,CAACN,YAAY,EAAE;QACrB,IAAI,IAAI,CAACD,YAAW,KAAM,gBAAgB,EAAE;UACxC,IAAI,CAACI,MAAM,EAAE;QACjB;QACA,IAAI,CAACJ,YAAW,GAAI,gBAAgB;MACxC,OAAO;QACH,IAAI,CAACA,YAAW,GAAI,aAAa;MACrC;IAEJ;EACJ;AACJ","names":["ViewBillResult","ViewBillForm","NoDataFound","EmptyResult","name","components","data","renderResult","selectedBill","selectedCategories","methods","reload","$forceUpdate","getQueryEvent","kwargs"],"sourceRoot":"","sources":["/home/petz/Meus projetos/digital-bill-vue/src/views/ViewBill.vue"],"sourcesContent":["<template>\n    <div>\n        <ViewBillForm @getQueryEvent=\"getQueryEvent\" />\n        <div id=\"view-bill-result\" class=\"row\">\n            <component :key=\"pageReload\" :is=\"renderResult\" :billTag=\"selectedBill\" :categoriesIgnoreTags=\"selectedCategories\"></component>\n        </div>\n    </div>\n</template>\n\n<script>\nimport ViewBillResult from \"../components/report/ViewBillResult\"\nimport ViewBillForm from \"../components/forms/ViewBillForm\"\nimport NoDataFound from \"../components/empty/NoDataFound.vue\"\nimport EmptyResult from \"../components/empty/EmptyResult.vue\"\n\nexport default {\n    name: 'ViewBill',\n    components: {\n        ViewBillResult, ViewBillForm, NoDataFound, EmptyResult\n    },\n    data() {\n        return {\n            renderResult: null,\n            selectedBill: null,\n            selectedCategories: null,\n        }\n    },\n    methods: {\n        reload() {\n            this.$forceUpdate();\n        },\n        getQueryEvent(kwargs) {\n            this.selectedBill = kwargs.selectedBill;\n            this.selectedCategories = kwargs.selectedCategories;\n\n\n            if (kwargs.selectedBill) {\n                if (this.renderResult === \"ViewBillResult\") {\n                    this.reload();\n                }\n                this.renderResult = \"ViewBillResult\";\n            } else {\n                this.renderResult = \"EmptyResult\";\n            }\n            \n        },\n    }\n}\n</script>\n\n<style>\nmain.mt-auto {\n    margin-top: 10% !important;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}