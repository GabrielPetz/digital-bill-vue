{"ast":null,"code":"import axios from \"axios\";\naxios.defaults.headers.post['Content-Type'] = 'application/json;charset=utf-8';\nexport default {\n  name: 'ViewBill',\n  props: {\n    bills: Array,\n    categories: Array\n  },\n  data() {\n    return {\n      bills: NormalModuleReplacementPlugin,\n      categories\n    };\n  },\n  methods: {\n    query() {\n      console.log(\"aquio\");\n      let bills = axios.get(\"http://localhost:8001/api/bill/\");\n      console.log(bills);\n    },\n    getBills() {\n      return axios({\n        method: 'get',\n        url: 'http://localhost:8001/api/bill/',\n        withCredentials: false,\n        responseType: 'json'\n      });\n    },\n    getCategories() {\n      return axios({\n        method: 'get',\n        url: 'http://localhost:8001/api/category/',\n        responseType: 'json'\n      });\n    }\n  },\n  async created() {\n    Promise.all([this.getBills(), this.getCategories()]).then(function (results) {\n      this.bills = results[0];\n      this.categories = results[1];\n      console.log(bills, categories);\n    });\n  }\n};","map":{"version":3,"mappings":"AAyBA,OAAOA,KAAI,MAAO,OAAO;AACzBA,KAAK,CAACC,QAAQ,CAACC,OAAO,CAACC,IAAI,CAAC,cAAc,IAAI,gCAAgC;AAE9E,eAAe;EACXC,IAAI,EAAE,UAAU;EAChBC,KAAK,EAAE;IACHC,KAAK,EAAEC,KAAK;IACZC,UAAU,EAAED;EAChB,CAAC;EACDE,IAAI,GAAG;IACH,OAAO;MACHH,KAAK,EAAEI,6BAA6B;MACpCF;IACJ;EACJ,CAAC;EACDG,OAAO,EAAE;IACLC,KAAK,GAAG;MACJC,OAAO,CAACC,GAAG,CAAC,OAAO;MACnB,IAAIR,KAAI,GAAIN,KAAK,CAACe,GAAG,CAAC,iCAAiC;MACvDF,OAAO,CAACC,GAAG,CAACR,KAAK;IACrB,CAAC;IACDU,QAAQ,GAAG;MACP,OAAOhB,KAAK,CAAC;QACTiB,MAAM,EAAE,KAAK;QACbC,GAAG,EAAE,iCAAiC;QACtCC,eAAe,EAAE,KAAK;QACtBC,YAAY,EAAE;MAClB,CAAC;IACL,CAAC;IACDC,aAAa,GAAG;MACZ,OAAOrB,KAAK,CAAC;QACTiB,MAAM,EAAE,KAAK;QACbC,GAAG,EAAE,qCAAqC;QAC1CE,YAAY,EAAE;MAClB,CAAC;IACL;EACJ,CAAC;EACD,MAAME,OAAO,GAAG;IACZC,OAAO,CAACC,GAAG,CAAC,CAAC,IAAI,CAACR,QAAQ,EAAE,EAAE,IAAI,CAACK,aAAa,EAAE,CAAC,EAC9CI,IAAI,CAAC,UAAUC,OAAO,EAAE;MACrB,IAAI,CAACpB,KAAI,GAAIoB,OAAO,CAAC,