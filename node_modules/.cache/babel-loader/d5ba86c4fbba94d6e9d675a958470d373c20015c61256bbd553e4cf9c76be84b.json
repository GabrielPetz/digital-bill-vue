{"ast":null,"code":"import axios from \"axios\";\nimport ViewBillResult from \"../components/report/ViewBillResult\";\nimport ViewBillForm from \"../components/forms/ViewBillForm\";\n// import NoDataFound from \"../components/empty/NoDataFound.vue\"\n// import EmptyResult from \"../components/empty/EmptyResult.vue\"\n\nexport default {\n  name: 'ViewBill',\n  components: {\n    ViewBillForm,\n    ViewBillResult //  NoDataFound, EmptyResult\n  },\n\n  data() {\n    return {\n      renderResult: null,\n      selectedBill: null,\n      selectedCategories: null,\n      apiResponse: null\n    };\n  },\n  methods: {\n    async getData() {\n      return new Promise(resolve => {\n        const response = axios({\n          method: 'get',\n          url: `http://localhost:8001/api/bill_statistics/?tag=${this.selectedBill}`,\n          withCredentials: false,\n          responseType: 'json'\n        }).then(response => {\n          this.apiResponse = response.data;\n        });\n      });\n    },\n    getQueryEvent(kwargs) {\n      this.selectedBill = kwargs.selectedBill;\n      this.selectedCategories = kwargs.selectedCategories;\n      this.renderResult = null;\n      if (kwargs.selectedBill) {\n        this.renderResult = \"ViewBillResult\";\n        this.getData();\n      } else {\n        this.renderResult = \"EmptyResult\";\n      }\n    }\n  }\n};","map":{"version":3,"mappings":"AAWA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,cAAa,MAAO,qCAAoC;AAC/D,OAAOC,YAAW,MAAO,kCAAiC;AAC1D;AACA;;AAEA,eAAe;EACXC,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE;IACRF,YAAY;IAAED,cAAc,CAAC;EACjC,CAAC;;EACDI,IAAI,GAAG;IACH,OAAO;MACHC,YAAY,EAAE,IAAI;MAClBC,YAAY,EAAE,IAAI;MAClBC,kBAAkB,EAAE,IAAI;MACxBC,WAAW,EAAE;IACjB;EACJ,CAAC;EACDC,OAAO,EAAE;IACL,MAAMC,OAAO,GAAG;MACZ,OAAO,IAAIC,OAAO,CAAEC,OAAO,IAAK;QAC5B,MAAMC,QAAO,GAAId,KAAK,CAAC;UACnBe,MAAM,EAAE,KAAK;UACbC,GAAG,EAAG,kDAAiD,IAAI,CAACT,YAAa,EAAC;UAC1EU,eAAe,EAAE,KAAK;UACtBC,YAAY,EAAE;QAClB,CAAC,CAAC,CAACC,IAAI,CAACL,QAAO,IAAK;UAEhB,IAAI,CAACL,WAAU,GAAIK,QAAQ,CAAC